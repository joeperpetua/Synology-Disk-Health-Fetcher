def initDisk(disk):
    disk_template = {}
    for attribute in disk:
        disk_template[attribute[0]] = attribute[1]
    return disk_template

    
current_disk = initDisk([
    ["vendor", disk['vendor']],
    ["model", disk['model']],
    ["serial", disk['serial']],
    ["slot", disk['slot']],
    ["path", disk['path']],
    ["overview_status", disk['overview_status']],
    ["smart_status", disk['smart']],
    ["smart_attr", disk['smart_attr']],
    ["smart_test_log", disk['smart_test_log']],
